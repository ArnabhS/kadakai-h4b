{
  "name": "kadakai-white-hat-agent",
  "displayName": "Kadakai White Hat Agent",
  "description": "Scan Web2/Web3 code and wallets for vulnerabilities. Integrates with Kadakai web app.",
  "version": "0.0.1",
  "publisher": "kadakai",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:kadakai.enterApiKey",
    "onCommand:kadakai.scanWorkspace"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kadakai.enterApiKey",
        "title": "Kadakai: Enter API Key"
      },
      {
        "command": "kadakai.scanWorkspace",
        "title": "Kadakai: Scan Workspace for Vulnerabilities"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.6.2",
    "typescript": "^4.7.0",
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
} 